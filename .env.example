# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here
# Optional OpenAI project (for sk-proj- keys)
# OPENAI_PROJECT=your_openai_project_id
# Optional model override; defaults to gpt-5-2025-08-07 if unset
# OPENAI_MODEL=gpt-5-2025-08-07
# Optional audio transcription model; defaults to gpt-4o-mini-transcribe
# OPENAI_TRANSCRIBE_MODEL=gpt-4o-mini-transcribe

# Anthropic Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key_here
# Optional Claude model override; defaults to claude-opus-4-1-20250805 if unset
# CLAUDE_MODEL=claude-opus-4-1-20250805

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# AWS (for Claude Batch API result downloads)
AWS_REGION=us-west-2
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# Next.js
NODE_ENV=development
