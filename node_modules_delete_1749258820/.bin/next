#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/brandonbrewer/Documents/Linguosity/Linguosity/node_modules/.pnpm/next@15.3.2_@babel+core@7.27.1_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-d_f1f7e3d0433b7d65d760107d9e0040be/node_modules/next/dist/bin/node_modules:/Users/brandonbrewer/Documents/Linguosity/Linguosity/node_modules/.pnpm/next@15.3.2_@babel+core@7.27.1_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-d_f1f7e3d0433b7d65d760107d9e0040be/node_modules/next/dist/node_modules:/Users/brandonbrewer/Documents/Linguosity/Linguosity/node_modules/.pnpm/next@15.3.2_@babel+core@7.27.1_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-d_f1f7e3d0433b7d65d760107d9e0040be/node_modules/next/node_modules:/Users/brandonbrewer/Documents/Linguosity/Linguosity/node_modules/.pnpm/next@15.3.2_@babel+core@7.27.1_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-d_f1f7e3d0433b7d65d760107d9e0040be/node_modules:/Users/brandonbrewer/Documents/Linguosity/Linguosity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/brandonbrewer/Documents/Linguosity/Linguosity/node_modules/.pnpm/next@15.3.2_@babel+core@7.27.1_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-d_f1f7e3d0433b7d65d760107d9e0040be/node_modules/next/dist/bin/node_modules:/Users/brandonbrewer/Documents/Linguosity/Linguosity/node_modules/.pnpm/next@15.3.2_@babel+core@7.27.1_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-d_f1f7e3d0433b7d65d760107d9e0040be/node_modules/next/dist/node_modules:/Users/brandonbrewer/Documents/Linguosity/Linguosity/node_modules/.pnpm/next@15.3.2_@babel+core@7.27.1_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-d_f1f7e3d0433b7d65d760107d9e0040be/node_modules/next/node_modules:/Users/brandonbrewer/Documents/Linguosity/Linguosity/node_modules/.pnpm/next@15.3.2_@babel+core@7.27.1_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-d_f1f7e3d0433b7d65d760107d9e0040be/node_modules:/Users/brandonbrewer/Documents/Linguosity/Linguosity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
